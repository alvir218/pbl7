    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Arquivo Atual",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        
        {
            "name": "django runserver error",
            "type": "python",
            "request": "launch",
            "program": "${file} runserver",
            "console": "integratedTerminal"
        },

        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
            ],
            "django": true
        }
    ]
}